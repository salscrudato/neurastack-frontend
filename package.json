{
  "name": "neurastack-frontend",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "cross-env VITE_APP_VERSION=$(date +%s) tsc -b && vite build",
    "build:prod": "cross-env VITE_APP_VERSION=$(date +%s) npm run clean && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && node scripts/bundle-analysis.js",
    "delete-firestore-data": "tsx scripts/delete-firestore-data.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/styled-system": "^2.12.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.8.1",
    "framer-motion": "^10.18.0",
    "nanoid": "^5.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.23.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^6.0.1",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.0"
  }
}
